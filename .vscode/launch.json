{
    "version": "0.2.0",
    "configurations": [
    {
    "name": "WSL Debug",
    "type": "cppdbg",
    "request": "launch",
    "program": "${workspaceFolder}/clearvision",
    "args": ["enc", "sample_io/secret-message-encrpytion/puppy.png", "hello"],
    "stopAtEntry": false,
    "cwd": "${workspaceFolder}",
    "environment": [],
    "externalConsole": false,
    "MIMode": "gdb",
    "miDebuggerPath": "/usr/bin/gdb",
    "setupCommands": [
    {
    "description": "Enable pretty-printing for gdb",
    "text": "-enable-pretty-printing",
    "ignoreFailures": true
    }
    ]
    }
    ]
}
